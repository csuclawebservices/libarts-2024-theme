{
	"name": "libarts-2024-theme",
	"version": "0.1.0",
	"description": "The block theme for all College of Liberal Arts websites",
	"author": "csuclawebservices",
	"keywords": [
		"WordPress",
		"theme"
	],
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build:editor": "wp-scripts build --webpack-src-dir=src/editor --output-path assets/js --output-filename editor.js",
		"build": "wp-scripts build --webpack-src-dir=src/blocks --output-path=blocks",
		"start:editor": "wp-scripts start --webpack-src-dir=src/editor --output-path assets/js --output-filename editor.js",
		"start": "wp-scripts start --webpack-src-dir=src/blocks --output-path=blocks"
	},
	"devDependencies": {
		"@csuclawebservices/icons": "^1.6.0",
		"@wordpress/icons": "^10.2.0",
		"@wordpress/scripts": "^28.2.0"
	}
}
