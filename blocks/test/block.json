{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cla/test",
  "version": "0.0.1",
  "title": "Test",
  "category": "theme",
  "icon": "admin-users",
  "description": "A test block.",
  "supports": {
    "align": false,
    "color": {
      "enableContrastChecker": true,
      "background": true,
      "gradients": true,
      "link": false,
      "text": true
    },
    "customClassName": true,
    "dimensions": {
      "minHeight": true
    },
    "html": false,
    "interactivity": true,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": false
    }
  },
  "textdomain": "cla",
  "editorScript": "file:./block.js",
  "viewScript": "file:./view.js",
  "viewScriptModule": "",
  "style": [
    "file:./style-block.css"
  ],
  "allowedBlocks": [
    "core/image",
    "core/video"
  ],
  "attributes": {
    "first_name": {
      "type": "string",
      "default": "default"
    },
    "toggle": {
      "type": "boolean",
      "default": false
    },
    "heading": {
      "type": "string",
      "selector": "h2",
      "source": "html"
    },
    "content": {
      "type": "string",
      "selector": "p",
      "source": "html"
    }
  }
}